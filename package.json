{
  "name": "webrtc-shooter",
  "version": "0.0.1",
  "description": "A short experiment on making peer to peer game with webrtc and impactjs",
  "type": "module",
  "scripts": {
    "prod": "pm2 start signalling/server.js --exp-backoff-restart-delay=100 -i 1 --name signalling && pm2 start server.js --exp-backoff-restart-delay=100",
    "sg-stop": "pm2 delete signalling",
    "signalling": "node signalling/server.js",
    "http": " http-server -c-1 . -p 8080",
    "dev:si": "nodemon signalling/server.js",
    "dev:se": "nodemon server.js",
    "prod:bot": "pm2 start bot.js --exp-backoff-restart-delay=100 --name telegram_bot_ff"
  },
  "keywords": [
    "webrtc",
    "p2p",
    "games",
    "javascript"
  ],
  "author": "alx.gutnikov@gmail.com",
  "devDependencies": {
    "http-server": "^0.9.0",
    "nodemon": "^2.0.19",
    "socket.io": "^1.4.8"
  },
  "jscsConfig": {
    "preset": "google",
    "maxErrors": 100,
    "maximumLineLength": 100,
    "validateIndentation": 2
  },
  "dependencies": {
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.2",
    "express": "^4.18.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.0",
    "node-telegram-bot-api": "^0.66.0",
    "pg": "^8.8.0",
    "redis": "^4.3.1",
    "ton": "^13.9.0",
    "ton-core": "^0.53.0",
    "ton-crypto": "^3.2.0",
    "tonweb": "^0.0.50",
    "web3": "^1.7.5"
  }
}
